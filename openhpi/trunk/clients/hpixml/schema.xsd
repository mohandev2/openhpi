</xs:simpleType>

<xs:simpleType name="SaHpiFloat64T">
 <xs:restriction base="xs:double"/>
</xs:simpleType>

<!-- SaHpiBoolT we derive from xs:boolean instead of SaHpiUint8T -->
<xs:simpleType name="SaHpiBoolT">
