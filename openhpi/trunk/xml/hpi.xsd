<!-- Basic HPI Data Types -->

<xsd:complexType name="SaHpiUint8Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:unsignedByte">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiUint8T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiUint16Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:unsignedShort">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiUint16T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiUint32Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:unsignedInt">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiUint32T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiUint64Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:unsignedLong">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiUint64T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiInt8Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:byte">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiInt8T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiInt16Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:short">
