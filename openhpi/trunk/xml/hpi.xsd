      <xsd:enumeration value="SAHPI_CAPABILITY_SENSOR" />
   </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="SaHpiHsCapabilitiesT">
   <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0" />
      <xsd:enumeration value="SAHPI_HS_CAPABILITY_AUTOEXTRACT_READ_ONLY" />
      <xsd:enumeration value="SAHPI_HS_CAPABILITY_INDICATOR_SUPPORTED" />
   </xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="SaHpiRptEntryType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="EntryId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="ResourceId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="ResourceInfo" type="hpi:SaHpiResourceInfoType" />
      <xsd:element name="ResourceEntity" type="hpi:SaHpiEntityPathType" />
      <xsd:element name="ResourceCapabilities">
         <xsd:simpleType>
            <xsd:list itemType="hpi:SaHpiCapabilitiesT" />
         </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HotSwapCapabilities">
         <xsd:simpleType>
            <xsd:list itemType="hpi:SaHpiHsCapabilitiesT" />
         </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ResourceSeverity" type="hpi:SaHpiSeverityT" />
      <xsd:element name="ResourceFailed" type="hpi:SaHpiBooleanType" />
      <xsd:element name="ResourceTag" type="hpi:SaHpiTextBufferType" />
   </xsd:sequence>
   <xsd:attribute name="name" use="required" />
   <xsd:attribute name="comment" use="optional" />
</xsd:complexType>

<!-- Domains -->

<xsd:simpleType name="SaHpiDomainCapabilitiesT">
   <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0" />
      <xsd:enumeration value="SAHPI_DOMAIN_CAP_AUTOINSERT_READ_ONLY" />
   </xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="SaHpiDomainInfoType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="DomainId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DomainCapabilities" type="hpi:SaHpiDomainCapabilitiesT" />
      <xsd:element name="IsPeer" type="hpi:SaHpiBooleanType" />
      <xsd:element name="DomainTag" type="hpi:SaHpiTextBufferType" />
      <xsd:element name="DrtUpdateCount" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DrtUpdateTimestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="RptUpdateCount" type="hpi:SaHpiUint32Type" />
      <xsd:element name="RptUpdateTimestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="DatUpdateCount" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DatUpdateTimestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="ActiveAlarms" type="hpi:SaHpiUint32Type" />
      <xsd:element name="CriticalAlarms" type="hpi:SaHpiUint32Type" />
      <xsd:element name="MajorAlarms" type="hpi:SaHpiUint32Type" />
      <xsd:element name="MinorAlarms" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DatUserAlarmLimit" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DatOverflow" type="hpi:SaHpiBooleanType" />
      <xsd:sequence minOccurs="1" maxOccurs="16">
         <xsd:element name="Guid" type="hpi:SaHpiUint8Type" />
      </xsd:sequence>
   </xsd:sequence>
   <xsd:attribute name="name" use="optional" default="myDomainInfo" />
</xsd:complexType>

<xsd:complexType name="SaHpiDrtEntryType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="EntryId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="DomainId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="IsPeer" type="hpi:SaHpiBooleanType" />
   </xsd:sequence>
   <xsd:attribute name="name" use="optional" default="myDrtEntry" />
</xsd:complexType>

<xsd:complexType name="SaHpiAlarmType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="AlarmId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="Timestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="Severity" type="hpi:SaHpiSeverityT" />
      <xsd:element name="Acknowledged" type="hpi:SaHpiBooleanType" />
      <xsd:element name="AlarmCond" type="hpi:SaHpiConditionType" />
   </xsd:sequence>
   <xsd:attribute name="name" use="optional" default="myAlarm" />
</xsd:complexType>

<!-- Event Log -->

<xsd:simpleType name="SaHpiEventLogOverflowActionT">
   <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SAHPI_EL_OVERFLOW_DROP" />
      <xsd:enumeration value="SAHPI_EL_OVERFLOW_OVERWRITE" />
   </xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="SaHpiEventLogInfoType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="Entries" type="hpi:SaHpiUint32Type" />
      <xsd:element name="Size" type="hpi:SaHpiUint32Type" />
      <xsd:element name="UserEventMaxSize" type="hpi:SaHpiUint32Type" />
      <xsd:element name="UpdateTimestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="CurrentTime" type="hpi:SaHpiInt64Type" />
      <xsd:element name="Enabled" type="hpi:SaHpiBooleanType" />
      <xsd:element name="OverflowFlag" type="hpi:SaHpiBooleanType" />
      <xsd:element name="OverflowResetable" type="hpi:SaHpiBooleanType" />
      <xsd:element name="OverflowAction" type="hpi:SaHpiEventLogOverflowActionT" />
   </xsd:sequence>
   <xsd:attribute name="name" use="optional" default="myEventLogInfo" />
</xsd:complexType>

<xsd:complexType name="SaHpiEventLogEntryType">
   <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element name="EntryId" type="hpi:SaHpiUint32Type" />
      <xsd:element name="Timestamp" type="hpi:SaHpiInt64Type" />
      <xsd:element name="Event" type="hpi:SaHpiEventType" />
   </xsd:sequence>
   <xsd:attribute name="name" use="optional" default="myEventLogEntry" />
</xsd:complexType>

</xsd:schema>
