         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiInt16T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiInt32Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:int">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiInt32T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiInt64Type">
   <xsd:simpleContent>
      <xsd:extension base="xsd:long">
         <xsd:attribute name="name" type="xsd:string" use="optional" />
         <xsd:attribute name="type" type="xsd:string" use="optional" default="SaHpiInt64T" />
      </xsd:extension>
   </xsd:simpleContent>
</xsd:complexType>

<xsd:complexType name="SaHpiFloat64Type">
   <xsd:simpleContent>
